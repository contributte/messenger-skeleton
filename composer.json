{
  "name": "contributte/messenger-skeleton",
  "description": "Example of integration of Symfony Messenger to Nette (@nette) by @f3l1x",
  "keywords": [
    "nette",
    "skeleton",
    "project",
    "messenger",
    "symfony"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=8.1",
    "contributte/nella": "^0.1",

    "contributte/messenger": "^0.1.0",
    "contributte/console": "^0.10.0",
    "contributte/event-dispatcher": "^0.9.0",

    "nettrine/annotations": "^0.7.0",
    "nettrine/orm": "^0.8.0",
    "nettrine/dbal": "^0.8.0",
    "nettrine/cache": "^0.3.0",
    "nettrine/migrations": "^0.8.0",
    "nettrine/fixtures": "^0.6.0",
    "contributte/pdf": "^6.1",

    "symfony/redis-messenger": "^6.2.10",
    "symfony/doctrine-messenger": "^6.2.10",
    "symfony/amqp-messenger": "^6.2.8"
  },
  "require-dev": {
    "contributte/qa": "^0.4.0",
    "contributte/dev": "^0.4.0",
    "phpstan/phpstan": "^1.4.3",
    "phpstan/phpstan-deprecation-rules": "^1.0.0",
    "phpstan/phpstan-nette": "^1.0.0",
    "phpstan/phpstan-strict-rules": "^1.1.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  }
}
